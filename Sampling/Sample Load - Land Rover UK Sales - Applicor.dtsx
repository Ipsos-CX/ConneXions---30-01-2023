                      dataType="System.String"><![CDATA[/* Microsoft SQL Server Integration Services Script Component
*  Write scripts using Microsoft Visual C# 2008.
*  ScriptMain is the entry point class of the script.*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.Windows.Forms;

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        try
        {
            if (!Row.UniqueID_IsNull && Row.UniqueID.Trim().Length > 0)
            {
                Row.CustomerIdentifier = Row.UniqueID;
                Row.CustomerIdentifierUsable = false ;
                //Row.CustomerIdentifierOriginatorPartyID = 6455217; // TODO: change this from a hard coded value
            }
            else
            {
                Row.CustomerIdentifierUsable = false;
            }

            /*
            if (!Row.txtAdd2_IsNull && Row.txtAdd2.Trim().Length > 0 && !Row.txtAdd3_IsNull && Row.txtAdd3.Trim().Length > 0)
            {
                Row.Locality = Row.txtAdd2.Trim() + " " + Row.txtAdd3.Trim();
            }
            else if (!Row.txtAdd2_IsNull && Row.txtAdd2.Trim().Length > 0)
            {
                Row.Locality = Row.txtAdd2.Trim();
            }
            else if (!Row.txtAdd3_IsNull && Row.txtAdd3.Trim().Length > 0)
            {
                Row.Locality = Row.txtAdd3.Trim();
            }
            */

            /* check address fields, if they contain any text concatenate as locality */
            string Add2 = Row.txtAdd2 ?? "";
            string Add3 = Row.txtAdd3 ?? "";
            string Locality = Add2.Trim() + " " + Add3.Trim();
            Row.Locality = Locality.Trim();
            
            
            if (!Row.VIN_IsNull && Row.VIN.Length == 17)
            {
                Row.VehicleIdentificationNumberUsable = true;
            }
            else
            {
                Row.VehicleIdentificationNumberUsable = false;
            }

            if (Row.OwnershipCycle.ToString().Trim().Length == 0)
            {
                Row.OwnershipCycle = null;
            }

            if (Row.ModelYear.ToString().Trim().Length == 0)
            {
                Row.ModelYear = null;
            }


            /*
             Delcare variable for Supression
            */
            Row.ConvertedPartySuppression = false;
            Row.ConvertedEmailSuppression = false;
            Row.ConvertedPostalSuppression = false;
            Row.GenderID = 0;// 0- Unknown 1- Male and 2- Female

            if (!Row.Gender_IsNull && Row.Gender.Trim().ToString().ToLower() == "male")
            {
                Row.GenderID = 1;
            }
            else if (!Row.Gender_IsNull && Row.Gender.Trim().ToString().ToLower() == "female")
            {
                Row.GenderID = 2;
            }



            if (!Row.TotalSuppression_IsNull && Row.TotalSuppression.Trim().ToString().ToLower() == "true")
            {
                Row.ConvertedPartySuppression = true;
                Row.ConvertedEmailSuppression = true;
                Row.ConvertedPostalSuppression = true;
            }


            if (!Row.Email_IsNull && Row.Email.Trim().ToString().ToLower() == "opt-out")
            {
                Row.ConvertedEmailSuppression = true;
            }


            if (!Row.Mail_IsNull && Row.Mail.Trim().ToString().ToLower() == "opt-out")
            {
                Row.ConvertedPostalSuppression = true;
            }
            
            

            Row.ManufacturerPartyID = Int32.Parse(ReadOnlyVariables["ManufacturerPartyID"].Value.ToString());
            Row.SampleSupplierPartyID = Int32.Parse(ReadOnlyVariables["ManufacturerPartyID"].Value.ToString());
            Row.CountryID = Int16.Parse(ReadOnlyVariables["CountryID"].Value.ToString());
            Row.EventTypeID = Int16.Parse(ReadOnlyVariables["EventTypeID"].Value.ToString());
            Row.LanguageID = Int16.Parse(ReadOnlyVariables["LanguageID"].Value.ToString());
            Row.DealerCodeOriginatorPartyID = Int32.Parse(ReadOnlyVariables["DealerCodeOriginatorPartyID"].Value.ToString());
            Row.SetNameCapitalisation = Boolean.Parse(ReadOnlyVariables["SetNameCapitalisation"].Value.ToString());
        }
        catch (Exception e)
        {
            throw e;
        }

        
    }

}
