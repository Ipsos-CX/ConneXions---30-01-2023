using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.Windows.Forms;

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        try
        {
            if (!Row.EmailAddress1_IsNull && Row.EmailAddress1.Length > 1)
            {
                Row.ValidatedEmailAddress1 = Row.EmailAddress1;
            }
            else
            {
                Row.ValidatedEmailAddress1 = "";
            }

            if (!Row.EmailAddress2_IsNull && Row.EmailAddress2.Length > 1)
            {
                Row.ValidatedEmailAddress2 = Row.EmailAddress2;
            }
            else
            {
                Row.ValidatedEmailAddress2 = "";
            }

            if (!Row.VIN_IsNull && Row.VIN.Length == 17)
            {
                Row.VehicleIdentificationNumberUsable = true;
            }
            else
            {
                Row.VehicleIdentificationNumberUsable = false;
            }
            
            Row.ManufacturerPartyID = Int32.Parse(ReadOnlyVariables["ManufacturerPartyID"].Value.ToString());
            Row.SampleSupplierPartyID = Int32.Parse(ReadOnlyVariables["ManufacturerPartyID"].Value.ToString());
            Row.CountryID = Int16.Parse(ReadOnlyVariables["CountryID"].Value.ToString());
            Row.EventTypeID = Int16.Parse(ReadOnlyVariables["EventTypeID"].Value.ToString());
            Row.LanguageID = Int16.Parse(ReadOnlyVariables["LanguageID"].Value.ToString());
            Row.DealerCodeOriginatorPartyID = Int32.Parse(ReadOnlyVariables["DealerCodeOriginatorPartyID"].Value.ToString());
            Row.SetNameCapitalisation = Boolean.Parse(ReadOnlyVariables["SetNameCapitalisation"].Value.ToString());

            Row.opCompleteSupressuion = false;
            Row.opSupressionEmail = false;
            Row.opSupressionMail = false;
            Row.opSupressionPhone = false;


            if (Row.CompleteSuppression.ToLower() == "yes")
            {
                Row.opCompleteSupressuion = true;
                Row.opSupressionEmail = true;
                Row.opSupressionMail = true;
                Row.opSupressionPhone = true;
            }

            if (Row.SuppressionEmail.ToLower() == "yes")
            {
                Row.opSupressionEmail = true;
            }

            if (Row.SuppressionMail.ToLower() == "yes")
            {
                Row.opSupressionMail = true;
            }

            if (Row.SuppressionPhone.ToLower() == "yes")
            {
                Row.opSupressionPhone = true;
            }

            /* Set ModelYear and OwnershipCycle to be 0 if not populated */
            if (Row.ModelYear.Length == 0)
            {
                Row.ModelYear = "0";
            }
            if (Row.OwnershipCycle.Length == 0)
            {
                Row.OwnershipCycle = "0";
            }

            //Calculating DealerCode
            if (!Row.DealerCode_IsNull && Row.DealerCode.Length > 0)
            {
                Row.DealerCode = Row.DealerCode.ToString();
            }

            
            // Calculating Customer Unique ID 
            Row.opCustomerIdentifierUseable = false;
            Row.opCustomerIdentifierOriginatorPartyID = 0;
            Row.opCustomerIdentifier = "";

            if (!Row.CustomerUniqueID_IsNull && Row.CustomerUniqueID.Length > 0)
            {
                Row.opCustomerIdentifier = Row.CustomerUniqueID;
                //customer identifier cannot be used to uniquely identify a customer
                Row.opCustomerIdentifierUseable = false;
                Row.opCustomerIdentifierOriginatorPartyID = Int32.Parse(ReadOnlyVariables["ManufacturerPartyID"].Value.ToString());
            }
            

           

            /* IF address field 8 (country) not populated in incoming file set the opAddress8 field to the 
             Country variable (populate using CountrID var )   */
            if (Row.opAddress8_IsNull || Row.opAddress8.Length == 0)
            {
                Row.opAddress8 = ReadOnlyVariables["Country"].Value.ToString();
            }

            /* If address is empty populate street and postcode with 'unknown' */
            if ((Row.Address1_IsNull || Row.Address1.Length == 0)
                && (Row.Address2_IsNull || Row.Address2.Length == 0)
                && (Row.Address3_IsNull || Row.Address3.Length == 0)
                && (Row.Address4_IsNull || Row.Address4.Length == 0)
                && (Row.Address5_IsNull || Row.Address5.Length == 0)
                && (Row.Address6_IsNull || Row.Address6.Length == 0)
                && (Row.Address7_IsNull || Row.Address7.Length == 0))
            {
                Row.Address1 = "Unknown";
                Row.Address7 = "Unknown";
            }


        }
        catch (Exception e)
        {
            throw e;
        }

        
    }

}
