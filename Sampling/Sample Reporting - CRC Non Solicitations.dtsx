    [Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        #region VSTA generated code
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

        /*
		The execution engine calls this method when the task executes.
		To access the object model, use the Dts property. Connections, variables, events,
		and logging features are available as members of the Dts property as shown in the following examples.

		To reference a variable, call Dts.Variables["MyCaseSensitiveVariableName"].Value;
		To post a log entry, call Dts.Log("This is my log text", 999, null);
		To fire an event, call Dts.Events.FireInformation(99, "test", "hit the help message", "", 0, true);

		To use the connections collection use something like the following:
		ConnectionManager cm = Dts.Connections.Add("OLEDB");
		cm.ConnectionString = "Data Source=localhost;Initial Catalog=AdventureWorks;Provider=SQLNCLI10;Integrated Security=SSPI;Auto Translate=False;";

		Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
		
		To open Help, press F1.
	*/

        public void Main()
        {
            try
            {
                FileInfo partyNonSolicitations = new FileInfo(Dts.Connections["PartyNonSolicitations"].ConnectionString);
                string partyNonSolicitationsConnectionString = partyNonSolicitations.DirectoryName + @"\" + GetTimeStamp(DateTime.Now) + partyNonSolicitations.Name;
                Dts.Connections["PartyNonSolicitations"].ConnectionString = partyNonSolicitationsConnectionString;
                
                FileInfo postalAddressNonSolicitations = new FileInfo(Dts.Connections["PostalAddressNonSolicitations"].ConnectionString);
                string postalAddressNonSolicitationsConnectionString = postalAddressNonSolicitations.DirectoryName + @"\" + GetTimeStamp(DateTime.Now) + postalAddressNonSolicitations.Name;
                Dts.Connections["PostalAddressNonSolicitations"].ConnectionString = postalAddressNonSolicitationsConnectionString;
                
                FileInfo emailAddressNonSolicitations = new FileInfo(Dts.Connections["EmailAddressNonSolicitations"].ConnectionString);
                string emailAddressNonSolicitationsConnectionString = emailAddressNonSolicitations.DirectoryName + @"\" + GetTimeStamp(DateTime.Now) + emailAddressNonSolicitations.Name;
                Dts.Connections["EmailAddressNonSolicitations"].ConnectionString = emailAddressNonSolicitationsConnectionString;

                FileInfo emailWildcardBlacklist = new FileInfo(Dts.Connections["EmailWildcardBlacklist"].ConnectionString);
                string emailWildcardBlacklistConnectionString = emailWildcardBlacklist.DirectoryName + @"\" + GetTimeStamp(DateTime.Now) + emailWildcardBlacklist.Name;
                Dts.Connections["EmailWildcardBlacklist"].ConnectionString = emailWildcardBlacklistConnectionString;

                FileInfo specificEmailBlacklist = new FileInfo(Dts.Connections["SpecificEmailBlacklist"].ConnectionString);
                string specificEmailBlacklistConnectionString = specificEmailBlacklist.DirectoryName + @"\" + GetTimeStamp(DateTime.Now) + specificEmailBlacklist.Name;
                Dts.Connections["SpecificEmailBlacklist"].ConnectionString = specificEmailBlacklistConnectionString;
                
                

                // Write details of the connection strings to the log file
                for (int i = 0; i < Dts.Connections.Count; i++)
                {
                    Dts.Log(Dts.Connections[i].Name + ": " + Dts.Connections[i].ConnectionString, 0, null);
                }

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception e)
            {
                throw e; ;
            }
        }

        private string GetTimeStamp(DateTime dt)
        {
            string y, m, d;

            y = dt.Year.ToString();
            m = dt.Month.ToString();
            d = dt.Day.ToString();

            return y.PadLeft(2, '0') + m.PadLeft(2, '0') + d.PadLeft(2, '0') + "_";
        }
    }
}]]></ProjectItem>
