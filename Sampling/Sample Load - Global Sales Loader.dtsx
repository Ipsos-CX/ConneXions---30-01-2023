                bool bSuccess = true;

                // get the file name from the full path and name
                FileInfo sampleFile = new FileInfo(Dts.Variables["SampleFilePathAndName"].Value.ToString());

                string strExt = sampleFile.Extension;
                Dts.Variables["GeneratedFilePathAndName"].Value = Dts.Variables["SampleFilePathAndName"].Value.ToString().Replace(strExt, "_parsed" + strExt);

                using (TextFieldParser parser = new TextFieldParser(Dts.Variables["SampleFilePathAndName"].Value.ToString()))
                {
                    string[] currentRow = null;
                    parser.TextFieldType = Microsoft.VisualBasic.FileIO.FieldType.Delimited;
                    parser.SetDelimiters("|");
                    //parser.HasFieldsEnclosedInQuotes = true;
                    parser.HasFieldsEnclosedInQuotes = false;

                    using (StreamWriter writer = new StreamWriter(File.Open(Dts.Variables["GeneratedFilePathAndName"].Value.ToString(), FileMode.Create), Encoding.UTF8))
                    {

                        while (!parser.EndOfData)
                        {
                            //Processing row
                            try
                            {
                                currentRow = parser.ReadFields();

                                //REPLACE ANY EMBEDDED DOUBLE QUOTES
                                for (int i = 0; i < currentRow.Length; i++)
                                {
                                    string value = currentRow[i];
                                    currentRow[i] = value.Replace("\"", "");
                                }

                                writer.WriteLine(String.Join("|", currentRow));

                            }
                            catch (Microsoft.VisualBasic.FileIO.MalformedLineException ex)
                            {
                                Dts.Log(ex.Message, 0, null);
                                //bSuccess = false;
                            }
                        }
                    }
                }


                // get the contents of the file into a string variable
                string fileContents = File.ReadAllText(Dts.Variables["GeneratedFilePathAndName"].Value.ToString());

                //// now get the hash code for that string and add it to the FileChecksum variable
                //Dts.Variables["FileChecksum"].Value = fileContents.GetHashCode();

                using (SHA256 SHA256 = SHA256Managed.Create())
                {
                    using (FileStream fileStream = System.IO.File.OpenRead(Dts.Variables["GeneratedFilePathAndName"].Value.ToString()))
                    {
                        string result = "";
                        foreach (var hash in SHA256.ComputeHash(fileStream))
                        {
                            result += hash.ToString("x2");
                        }

                        Dts.Variables["FileChecksum"].Value = result;
                    }
                }

                // get the number of rows in the file
                string[] fileRows = File.ReadAllLines(Dts.Variables["GeneratedFilePathAndName"].Value.ToString());
                Dts.Variables["FileRowCount"].Value = fileRows.Length;

                // set the LoadSuccess flag to true - we change this later if it fails
                Dts.Variables["LoadSuccess"].Value = true;

                string CleanedSampleFileName = sampleFile.Name.ToString().Replace(strExt, "_parsed" + strExt);
                // log the variables
                Dts.Log("Original SampleFileName: " + sampleFile.Name, 0, null);
                Dts.Log("Cleaned SampleFileName: " + CleanedSampleFileName, 0, null);
                Dts.Log("FileRowCount: " + Dts.Variables["FileRowCount"].Value.ToString(), 0, null);
                Dts.Log("FileChecksum: " + Dts.Variables["FileChecksum"].Value.ToString(), 0, null);

                Dts.Variables["SampleFileName"].Value = CleanedSampleFileName;

                if (bSuccess)
                    Dts.TaskResult = (int)ScriptResults.Success;
                else
                    Dts.TaskResult = (int)ScriptResults.Failure;
            }

            catch (System.Exception e)
            {
                Dts.Log(e.Message, 0, null);
                Dts.TaskResult = (int)ScriptResults.Failure;
