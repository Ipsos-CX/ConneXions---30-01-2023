<![CDATA[/*
   Microsoft SQL Server Integration Services Script Task
   Write scripts using Microsoft Visual C# 2008.
   The ScriptMain is the entry point class of the script.
*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.IO;

namespace ST_a95b78bf3c054aa595723be19e228933.csproj
{
    [System.AddIn.AddIn("ScriptMain", Version = "1.0", Publisher = "", Description = "")]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        #region VSTA generated code
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

        /*
		The execution engine calls this method when the task executes.
		To access the object model, use the Dts property. Connections, variables, events,
		and logging features are available as members of the Dts property as shown in the following examples.

		To reference a variable, call Dts.Variables["MyCaseSensitiveVariableName"].Value;
		To post a log entry, call Dts.Log("This is my log text", 999, null);
		To fire an event, call Dts.Events.FireInformation(99, "test", "hit the help message", "", 0, true);

		To use the connections collection use something like the following:
		ConnectionManager cm = Dts.Connections.Add("OLEDB");
		cm.ConnectionString = "Data Source=localhost;Initial Catalog=AdventureWorks;Provider=SQLNCLI10;Integrated Security=SSPI;Auto Translate=False;";

		Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
		
		To open Help, press F1.
	*/

        public void Main()
        {
            // TODO: Add your code here

            /*
               Purpose:    - Using the full filepath the destinationpath gets created
             
              
               Version     Developer           Date            Comment
               1.0         Attila Kubanda      23/02/2012      Created
           */


            try
            {
                // check to ensure variable is accessible
                if (Dts.Variables.Contains("UV_sFileAndPath"))
                {
                    // put file name in a variable
                    string sFilePath = Dts.Variables["User::UV_sFileAndPath"].Value.ToString();

                    if (!File.Exists(sFilePath)) // check file exists
                    {
                        // MessageBox.Show("File does not exist");
                        Dts.TaskResult = (int)ScriptResults.Failure; // if not exit
                    }
                    else // if so, open file and count the rows
                        //MessageBox.Show(sFilePath);
                    {
                        //MessageBox.Show(Dts.Variables["UV_sFileAndPath"].Value.ToString());
                        //MessageBox.Show(Directory.GetParent(sFilePath).ToString());
                        string sFileParent = Directory.GetParent(sFilePath).ToString();

                        int i = sFileParent.LastIndexOf(@"\");

                        string subdirectory = sFileParent.Substring(i);

                        //MessageBox.Show(i.ToString());
                        //MessageBox.Show(subdirectory);

                        string fulldestinationpath = Dts.Variables["User::UV_DestinationFolder"].Value + subdirectory;
                        string fullarchivepath = Dts.Variables["User::UV_DestinationArchive"].Value + subdirectory;
                        
                        //MessageBox.Show(fulldestinationpath);

                        Dts.Variables["User::UV_TempDestinationFolder"].Value = fulldestinationpath;
                        Dts.Variables["User::UV_TempDestinationArchive"].Value = fullarchivepath;
                    }

                }
            }

            catch (Exception ex)
            {
                Dts.Events.FireError(0, "FELC_TraverseLoadDirectory", ex.Message + "\r" + ex.StackTrace, string.Empty, 0);
            }

            finally
            {

                Dts.TaskResult = (int)ScriptResults.Success;
            }

        }
    }
}]]></ProjectItem><ProjectItem Name="\properties\settings.designer.cs">
