using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    bool loadFailure = false;


    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        // set the values to be written to the Error table
        Row.ErrorDescription = this.ComponentMetaData.GetErrorDescription(Row.ErrorCode);
        //Row.ErrorColumnName = Row.ErrorColumn;
        Row.FileName = ReadOnlyVariables["SampleFileName"].Value.ToString();
        //Row.RowNumber = Row.PhysicalRow;
        Row.PackageName = ReadOnlyVariables["PackageName"].Value.ToString();
        Row.AuditID = Int64.Parse(ReadOnlyVariables["AuditID"].Value.ToString());

        if (Row.ErrorCode != 0)
        {
            loadFailure = true;
        }
    }

    public override void PostExecute()
    {
        base.PostExecute();
        // set the LoadSuccess flag so we can work out which process path to follow when 
        // control is passed back to the control flow.  
        if (loadFailure == true)
        {
            ReadWriteVariables["LoadSuccess"].Value = false;
        }
    }

}
