            Dts.Log("WinSCP Params - User::FTPDeleteRemoteFileAfterDownload: " + Dts.Variables["User::FTPDeleteRemoteFileAfterDownload"].Value.ToString(), 0, null);
            Dts.Log("WinSCP Params - User::FTPIncludeAllRemoteSubdirectories: " + Dts.Variables["User::FTPIncludeAllRemoteSubdirectories"].Value.ToString(), 0, null);
            Dts.Log("WinSCP Params - User::FTPStampFileNameWithRemoteDirName: " + Dts.Variables["User::FTPStampFileNameWithRemoteDirName"].Value.ToString(), 0, null);
            Dts.Log("WinSCP Params - User::FTPRemoteFileMask: " + Dts.Variables["User::FTPRemoteFileMask"].Value.ToString(), 0, null);
            Dts.Log("WinSCP Params - User::FTPTransferMode: " + Dts.Variables["User::FTPTransferMode"].Value.ToString(), 0, null);
            Dts.Log("WinSCP Params - User::WinSCPExecutablePath: " + Dts.Variables["User::WinSCPExecutablePath"].Value.ToString(), 0, null);
            Dts.Log("WinSCP Params - User::FTPLocalPath: " + Dts.Variables["User::FTPLocalPath"].Value.ToString(), 0, null);
            Dts.Log("WinSCP Params - User::SSHPrivateKeyLocation: " + Dts.Variables["User::SSHPrivateKeyLocation"].Value.ToString(), 0, null);
            Dts.Log("WinSCP Params - User::SSHPrivateKey: " + Dts.Variables["User::SSHPrivateKey"].Value.ToString(), 0, null);

            //PATH to the SSH private key
            string SSHPrivateKey = Dts.Variables["User::SSHPrivateKeyLocation"].Value + @"\" + Dts.Variables["User::SSHPrivateKey"].Value;

            // Setup session options
            SessionOptions sessionOptions = new SessionOptions
            {
                Protocol = Protocol.Sftp,
                SshHostKeyFingerprint = (string)Dts.Variables["User::FTPFingerprint"].Value,
               //GiveUpSecurityAndAcceptAnySshHostKey = true,   // use this instead to bypass HostKey fingerprint requirement
                HostName = (string)Dts.Variables["User::FTPHostName"].Value,
                UserName = (string)Dts.Variables["User::FTPUserName"].Value,
                //Password = (string)Dts.Variables["User::FTPPassword"].Value,      --V1.6 USE SSH PASSKEY AUTHENTICATION INSTEAD
                SshPrivateKeyPath = (string)SSHPrivateKey
            };

            bool remoteDeleteAfterDownload = (bool)Dts.Variables["User::FTPDeleteRemoteFileAfterDownload"].Value;
            bool remoteIncludeSubdirectories = (bool)Dts.Variables["User::FTPIncludeAllRemoteSubdirectories"].Value;
            bool stampFileNameWithRemoteDirName = (bool)Dts.Variables["User::FTPStampFileNameWithRemoteDirName"].Value;

            string remotePath = (string)Dts.Variables["User::FTPRemotePath"].Value;
            string remoteFileMask = (string)Dts.Variables["User::FTPRemoteFileMask"].Value;
            string transferMode = (string)Dts.Variables["User::FTPTransferMode"].Value;
            string sessionExecutablePath = (string)Dts.Variables["User::WinSCPExecutablePath"].Value;
            string localPath = (string)Dts.Variables["User::FTPLocalPath"].Value + "*.*_encrypted";

 
             try
            {
                using (Session session = new Session())
                {
                    // As WinSCP .NET assembly has to be stored in GAC to be used with SSIS,
                    // you need to set path to WinSCP.exe explicitly,
                    // if using non-default location.
                    session.ExecutablePath = sessionExecutablePath;
                   
                    // Connect
                    session.Open(sessionOptions);
 

                // Set transfer options 
                TransferOptions transferOptions = new TransferOptions();
                if (transferMode == "ascii")
                {
                    transferOptions.TransferMode = TransferMode.Ascii;
                }
                else
                {
                    transferOptions.TransferMode = TransferMode.Binary;
                }
                transferOptions.FileMask = remoteFileMask;
          
                   
                    EnumerationOptions passedEnumerationOptions = new EnumerationOptions();

                    if (remoteIncludeSubdirectories)
                    {
                        passedEnumerationOptions = EnumerationOptions.EnumerateDirectories | EnumerationOptions.AllDirectories;
                    }
                    else
                    {
                        passedEnumerationOptions = EnumerationOptions.None;
                    }

                // Enumerate files to download
                IEnumerable<RemoteFileInfo> fileInfos =
                    session.EnumerateRemoteFiles(
                        remotePath, remoteFileMask,
                           passedEnumerationOptions);
 
                foreach (RemoteFileInfo fileInfo in fileInfos)
                {
                   if (!fileInfo.IsDirectory)   //We ignore directory entries
                    {
                        Console.WriteLine("Downloading file {0}...", fileInfo.FullName);
                        Dts.Log("WinSCP - Downloading file: " + fileInfo.FullName, 0, null);
                  
                       // Download file
                        string remoteFilePath = RemotePath.EscapeFileMask(fileInfo.FullName);

                        var remoteDirName = new DirectoryInfo(remoteFilePath).Parent.Name.Replace("Ipsos", "");  // Get the parent directory name minus the "Ipsos" to use a file stamp

                        if (stampFileNameWithRemoteDirName == true)
                        {
                            localPath = (string)Dts.Variables["User::FTPLocalPath"].Value + "*_" + remoteDirName + ".*_encrypted";
                        }

                        Dts.Log("WinSCP variable - localPath: " + localPath, 0, null);
 
                       TransferOperationResult transferResult =
                          session.GetFiles(remoteFilePath, localPath, remoteDeleteAfterDownload, transferOptions);

                        // Did the download succeeded?
                        if (!transferResult.IsSuccess)
                        {
                            // Print error (but continue with other files)
                            Console.WriteLine(
                                "Error downloading file {0}: {1}",
                                fileInfo.FullName, transferResult.Failures[0].Message);

                            Dts.Log("Error downloading file: " + transferResult.Failures[0].Message, 0, null);
 
                        }
                    }
                 }
            }

                Dts.TaskResult = (int)DTSExecResult.Success;
            }
            catch (Exception e)
            {
                Dts.Events.FireError(0, null,
                    string.Format("Error when using WinSCP to download files: {0}", e),
                    null, 0);

                Dts.TaskResult = (int)DTSExecResult.Failure;
            }

        }
    }
}]]></ProjectItem>
