        public void Main()
        {
            try
            {
                // disable the steps that output the sales and service files
                // we will reenable the correct one shortly
                Dts.Variables["DisablePostalSales"].Value = true;
                Dts.Variables["DisablePostalService"].Value = true;
                Dts.Variables["DisablePostalPreOwned"].Value = true;
                Dts.Variables["DisablePostalCRC"].Value = true;
                Dts.Variables["DisablePostalBodyshop"].Value = true;

                // check if we've got any data to output
                if (Int32.Parse(Dts.Variables["NumberPostalRows"].Value.ToString()) > 0)
                {
                    // calculate the number of batches we need
                    // use the number of rows in the postal table divided by the number in each batch ad round up to the next integer
                    int postalBatchCount = 0;
                    double doublePostalBatchCounter = 0;
                    double numberPostalRows = Double.Parse(Dts.Variables["NumberPostalRows"].Value.ToString());
                    double numberRowsInPostalBatch = Double.Parse(Dts.Variables["NumberRowsInPostalBatch"].Value.ToString());

                    doublePostalBatchCounter = numberPostalRows / numberRowsInPostalBatch;

                    postalBatchCount = Int32.Parse(Math.Ceiling(doublePostalBatchCounter).ToString());

                    Dts.Variables["PostalBatchCount"].Value = postalBatchCount;

                    // set the output file location and name
                    if (Dts.Variables["Questionnaire"].Value.ToString() == "Sales")
                    {
                        // set the flag to enalbe the sales postal output
                        Dts.Variables["DisablePostalSales"].Value = false;
                    }
                    else if (Dts.Variables["Questionnaire"].Value.ToString() == "Service")
                    {
                        // set the flag to disable the service postal output
                        Dts.Variables["DisablePostalService"].Value = false;
                    }
                    else if (Dts.Variables["Questionnaire"].Value.ToString() == "PreOwned")
                    {
                        // set the flag to disable the service postal output
                        Dts.Variables["DisablePostalPreOwned"].Value = false;
                    }
                    else if (Dts.Variables["Questionnaire"].Value.ToString() == "CRC")
                    {
                        // set the flag to disable the service postal output
                        Dts.Variables["DisablePostalCRC"].Value = false;
                    }
                    else if (Dts.Variables["Questionnaire"].Value.ToString() == "Bodyshop")
                    {
                        // set the flag to disable the service postal output
                        Dts.Variables["DisablePostalBodyshop"].Value = false;
                    }
                }

                Dts.Log("PostalBatchCount: " + Dts.Variables["PostalBatchCount"].Value.ToString(), 0, null);
                Dts.Log("NumberPostalRows: " + Dts.Variables["NumberPostalRows"].Value.ToString(), 0, null);
