                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">;with CTE_DeDuped_Events (eventID, AuditItemID)
AS 
	(
		SELECT ODSEventID , MAX(AuditItemID)
		from Sample_ETL.CRC.CRCEvents
		GROUP BY ODSEventID
	),

 CTE_Results ( [Password], ID, EventDate, fullModel, Model, SType, Carreg, Title, Initial, Surname, fullname,
				dearname, CoName, add1, add2, add3, add4, add5, add6, add7, add8, add9, CTRY, EmailAddress,
				Dealer, sno, ccode, modelcode, lang, manuf, gender, qver, surveyscale, etype, reminder,
				[week], test, sampleflag, CRCsurveyfile, ITYPE, [Owner], GfKPartyID)

AS
(SELECT DISTINCT
P.Password,
P.ID,
CAST(replace(convert(varchar(10), P.EventDate, 102), '.', '-') AS VARCHAR(10)) AS EventDate, 
P.fullModel,
P.Model,
P.SType,
P.Carreg,
P.Title,
P.Initial,
P.Surname,
P.fullname,
P.dearname,
P.CoName,
P.add1,
P.add2,
P.add3,
P.add4,
P.add5,
P.add6,
P.add7,
P.add8,
P.add9,
P.CTRY,
P.EmailAddress,
ISNULL(P.dealer, '') AS Dealer,
P.sno,
P.ccode,
P.modelcode,
P.lang,
P.manuf,
P.gender,
P.qver,
p.blank AS surveyscale,
P.etype,
P.reminder,
P.week,
P.test,
P.sampleflag,
P.SalesServiceFile AS CRCsurveyfile,
'' AS ITYPE,
--CASE 
--	WHEN LEN(ISNULL(CRC.ClosedBy,'')) &gt; 0 THEN SUBSTRING(CRC.ClosedBy, 1, ISNULL(NULLIF(CHARINDEX(' ', CRC.ClosedBy) - 1, -1),LEN(CRC.ClosedBy))) 		 
--	ELSE SUBSTRING(CRC.[Owner], 1, ISNULL(NULLIF(CHARINDEX(' ', CRC.[Owner]) - 1, -1),LEN(CRC.[Owner]))) 
--END AS [Owner],  -- BUG 12103 Output Owner field if ClosedBy is blank (And output first name if there)
CASE
	WHEN LK.CODE IS NOT NULL THEN LK.FirstName	-- WE'VE GOT A MATCH IN THE LOOKUP TABLE, USE IT. 
	WHEN LK.CODE IS NULL AND LEN(ISNULL(CRC.ClosedBy,'')) &gt; 0 THEN SUBSTRING(ltrim(CRC.ClosedBy), 1, ISNULL(NULLIF(CHARINDEX(' ', ltrim(CRC.ClosedBy)) - 1, -1),LEN(ltrim(CRC.ClosedBy))))
	ELSE SUBSTRING(ltrim(CRC.[Owner]), 1, ISNULL(NULLIF(CHARINDEX(' ', ltrim(CRC.[Owner])) - 1, -1),LEN(ltrim(CRC.[Owner]))))
END AS [Owner],
P.PartyID AS GfKPartyID
--INTO #TMP
FROM        SelectionOutput.Postal  P
INNER JOIN Sample.Event.AutomotiveEventBasedInterviews aebi ON p.ID = aebi.CaseID
INNER JOIN	 CTE_DeDuped_Events red on red.EventID = aebi.EventID
INNER JOIN   Sample_ETL.CRC.CRCEvents crc ON crc.AuditItemID = red.AuditItemID
LEFT JOIN   Sample_ETL.[Lookup].[CRCAgentLookup] lk ON
			CASE 
				WHEN LEN(ISNULL(CRC.ClosedBy,'')) &gt; 0 THEN LTRIM(RTRIM(CRC.ClosedBy))
				ELSE LTRIM(RTRIM(CRC.[Owner])) 
			END  = lk.Code AND
			
			crc.BrandCode = CASE 
								 WHEN lk.Brand = 'Jaguar' THEN 'J'
								 WHEN lk.Brand = 'Land Rover' THEN 'L'
								 ELSE lk.Brand
							END
							
							AND
			crc.MarketCode =  lk.MarketCode
WHERE     (Outputted = 1)
)

