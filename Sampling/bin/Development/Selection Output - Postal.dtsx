                        Encoding="UTF8"><![CDATA[/*
   Microsoft SQL Server Integration Services Script Task
   Write scripts using Microsoft Visual C# 2008.
   The ScriptMain is the entry point class of the script.
*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.IO;

namespace ST_b6a2c483dd424970bc6aa21c83ef0568.csproj
{
    [Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        #region VSTA generated code
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

        /*
		The execution engine calls this method when the task executes.
		To access the object model, use the Dts property. Connections, variables, events,
		and logging features are available as members of the Dts property as shown in the following examples.

		To reference a variable, call Dts.Variables["MyCaseSensitiveVariableName"].Value;
		To post a log entry, call Dts.Log("This is my log text", 999, null);
		To fire an event, call Dts.Events.FireInformation(99, "test", "hit the help message", "", 0, true);

		To use the connections collection use something like the following:
		ConnectionManager cm = Dts.Connections.Add("OLEDB");
		cm.ConnectionString = "Data Source=localhost;Initial Catalog=AdventureWorks;Provider=SQLNCLI10;Integrated Security=SSPI;Auto Translate=False;";

		Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
		
		To open Help, press F1.
	*/

        public void Main()
        {
            try
            {
                // do some logging of variables passed in
                Dts.Log("LocalDownloadDirectory: " + Dts.Variables["LocalDownloadDirectory"].Value.ToString(), 0, null);
                Dts.Log("Brand: " + Dts.Variables["Brand"].Value.ToString(), 0, null);
                Dts.Log("Questionnaire: " + Dts.Variables["Questionnaire"].Value.ToString(), 0, null);
                Dts.Log("Market: " + Dts.Variables["Market"].Value.ToString(), 0, null);
                Dts.Log("FileNameTimeStamp: " + Dts.Variables["FileNameTimeStamp"].Value.ToString(), 0, null);
                Dts.Log("DataFileExtension: " + Dts.Variables["DataFileExtension"].Value.ToString(), 0, null);
                Dts.Log("FileNamePostfix: " + Dts.Variables["FileNamePostfix"].Value.ToString(), 0, null);
                Dts.Log("BilingualIdentifier: " + Dts.Variables["BilingualIdentifier"].Value.ToString(), 0, null);


                Dts.Log("LoopCounter: " + Dts.Variables["LoopCounter"].Value.ToString(), 0, null);

                // get the batch number as a two charater string
                string batch = Dts.Variables["LoopCounter"].Value.ToString().PadLeft(2, '0');

                // V1.3 set FileNamePostfixBilingual
                string FileNamePostfixBilingual = "";
                if (Dts.Variables["BilingualIdentifier"].Value.ToString() != "")
                {
                    FileNamePostfixBilingual = Dts.Variables["BilingualIdentifier"].Value.ToString() + '_';
                }

                // set the output file location and name
                string excelPath = Dts.Variables["LocalDownloadDirectory"].Value.ToString() +
                    @"\JLR_" +
                    Dts.Variables["Brand"].Value.ToString().Replace(" ", "") + "_" +
                    Dts.Variables["FileNamePostfix"].Value.ToString() + "_" +
                    FileNamePostfixBilingual +      // V1.3 Add Bilingual Identifier
                    Dts.Variables["Questionnaire"].Value.ToString() + "_" +
                    "Combined_PostalSelection_" +
                    Dts.Variables["FileNameTimeStamp"].Value.ToString() + "_" +
                    batch;



                //MessageBox.Show("Brand: " + Dts.Variables["Brand"].Value.ToString() + "Questionnaire: " + Dts.Variables["Questionnaire"].Value.ToString() + "Market: " + Dts.Variables["Market"].Value.ToString());

                // set output file name
                excelPath = excelPath + Dts.Variables["DataFileExtension"].Value.ToString();
                Dts.Log("Output File Path: " + excelPath, 0, null);

                // create the copy of the output file
                // get the template Excel file path
                string templatePath;

                templatePath = Dts.Variables["PostalServiceTemplatePath"].Value.ToString();


                FileInfo f = new FileInfo(excelPath);
                //if File doesn't exist we create it
                if (!f.Exists)
                {
                    // create a copy of the template with the new timestamped name
                    File.Copy(templatePath, excelPath);
                }


                // set the filename variable for use in auditing
                //FileInfo f = new FileInfo(excelPath);
                Dts.Variables["FileName"].Value = f.Name;

                // set the connection path to be the new timestamped file
                Dts.Connections["Postal Service Output"].ConnectionString = "Provider=Microsoft.ACE.OLEDB.12.0;Data Source=" + excelPath + ";Extended Properties=\"EXCEL 12.0;HDR=YES\"";

                // log the connection string
                Dts.Log("Output File CSV Connection String: " + Dts.Connections["Postal Service Output"].ConnectionString, 0, null);



                //MessageBox.Show("Output File CSV Connection String: " + Dts.Connections["Postal Service Output"].ConnectionString);
                
                Dts.TaskResult = (int)ScriptResults.Success;
            }

            catch (Exception e)
            {
                Dts.Log(e.Message, 0, null);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }
    }
}]]></ProjectItem>
