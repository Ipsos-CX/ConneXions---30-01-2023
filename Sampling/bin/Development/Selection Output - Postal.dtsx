                Encoding="UTF8"><![CDATA[/*
   Microsoft SQL Server Integration Services Script Task
   Write scripts using Microsoft Visual C# 2008.
   The ScriptMain is the entry point class of the script.
*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;

namespace ST_ba1e46460c894ace936bf7a4d1b1fbe1.csproj
{
    [Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        #region VSTA generated code
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

        /*
		The execution engine calls this method when the task executes.
		To access the object model, use the Dts property. Connections, variables, events,
		and logging features are available as members of the Dts property as shown in the following examples.

		To reference a variable, call Dts.Variables["MyCaseSensitiveVariableName"].Value;
		To post a log entry, call Dts.Log("This is my log text", 999, null);
		To fire an event, call Dts.Events.FireInformation(99, "test", "hit the help message", "", 0, true);

		To use the connections collection use something like the following:
		ConnectionManager cm = Dts.Connections.Add("OLEDB");
		cm.ConnectionString = "Data Source=localhost;Initial Catalog=AdventureWorks;Provider=SQLNCLI10;Integrated Security=SSPI;Auto Translate=False;";

		Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
		
		To open Help, press F1.
	*/

        public void Main()
        {
            try
            {
                // disable the steps that output the sales and service files
                // we will reenable the correct one shortly
                //MessageBox.Show(Dts.Variables["DisablePostalSales"].Value.ToString());
                Dts.Variables["DisablePostalSales"].Value = true;
                Dts.Variables["DisablePostalServices"].Value = true;

                // check if we've got any data to output
                if (Int32.Parse(Dts.Variables["NumberPostalRows"].Value.ToString()) > 0)
                {
                    // calculate the number of batches we need
                    // use the number of rows in the postal table divided by the number in each batch ad round up to the next integer
                    int postalBatchCount = 0;
                    double doublePostalBatchCounter = 0;
                    double numberPostalRows = Double.Parse(Dts.Variables["NumberPostalRows"].Value.ToString());
                    double numberRowsInPostalBatch = Double.Parse(Dts.Variables["NumberRowsInPostalBatch"].Value.ToString());

                    doublePostalBatchCounter = numberPostalRows / numberRowsInPostalBatch;

                    postalBatchCount = Int32.Parse(Math.Ceiling(doublePostalBatchCounter).ToString());

                    Dts.Variables["PostalBatchCount"].Value = postalBatchCount;

                    // set the output file location and name
                    if (Dts.Variables["Questionnaire"].Value.ToString() == "Sales")
                    {
                        // set the flag to enalbe the sales postal output
                        Dts.Variables["DisablePostalSales"].Value = false;
                    }
                    else if (Dts.Variables["Questionnaire"].Value.ToString() == "Service")
                    {
                        // set the flag to disable the service postal output
                        Dts.Variables["DisablePostalServices"].Value = false;
                    }
  
                }

                Dts.Log("PostalBatchCount: " + Dts.Variables["PostalBatchCount"].Value.ToString(), 0, null);
                Dts.Log("NumberPostalRows: " + Dts.Variables["NumberPostalRows"].Value.ToString(), 0, null);
                Dts.Log("NumberRowsInPostalBatch: " + Dts.Variables["NumberRowsInPostalBatch"].Value.ToString(), 0, null);
                Dts.Log("DisablePostalSales: " + Dts.Variables["DisablePostalSales"].Value.ToString(), 0, null);
                Dts.Log("DisablePostalServices: " + Dts.Variables["DisablePostalServices"].Value.ToString(), 0, null);

                Dts.TaskResult = (int)ScriptResults.Success;
            
            }

            catch (Exception e)
            {
                Dts.Log(e.Message, 0, null);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }
    }
}]]></ProjectItem>
