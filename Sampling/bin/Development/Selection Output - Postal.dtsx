      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{F704349E-595B-4117-8991-D710C0C23B9B}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="SELECT DISTINCT SM.Brand, SM.Questionnaire, SM.Market, RG.Region, &#xA;CASE WHEN SM.PostalMergedType = 'Market' THEN SM.ISOAlpha3 &#xA;ELSE RG.RegionShortName END AS DataFilePostfix,&#xA;ISNULL(PC.BilingualFlag,'FALSE') AS BilingualFlag,&#xA;CASE WHEN ISNULL(PC.BilingualFlag,'FALSE') = 'TRUE' THEN L.ISOAlpha2 ELSE '' END AS BilingualIdentifier,&#xA;CASE WHEN ISNULL(PC.BilingualFlag,'FALSE') = 'TRUE' THEN L.LanguageID ELSE 0 END AS LanguageID&#xA;FROM SelectionOutput.PostalCombined PC&#xA;INNER JOIN Requirement.SelectionCases SC ON PC.ID = SC.CaseID&#xA;INNER JOIN Requirement.RequirementRollups RR ON SC.RequirementIDPartOf = RR.RequirementIDMadeUpOf&#xA;INNER JOIN Requirement.Requirements R ON RR.RequirementIDPartOf = R.RequirementID&#xA;INNER JOIN dbo.vwBrandMarketQuestionnaireSampleMetadata SM ON R.RequirementID = SM.QuestionnaireRequirementID&#xA;INNER JOIN dbo.Markets MK ON SM.CountryID = MK.CountryID&#xA;INNER JOIN dbo.Regions RG ON MK.RegionID = RG.RegionID&#xA;LEFT JOIN dbo.Languages L ON PC.lang = L.LanguageID&#xA;WHERE SM.PostalMerged = 1&#xA;AND&#x9;(PC.DateOutput IS NULL)&#xA;&#xA;ORDER BY RG.Region, SM.Questionnaire, SM.Brand"
          SQLTask:ResultType="ResultSetType_Rowset" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="0"
            SQLTask:DtsVariableName="User::SelectionsToOutput" />
        </SQLTask:SqlTaskData>
      </DTS:ObjectData>
