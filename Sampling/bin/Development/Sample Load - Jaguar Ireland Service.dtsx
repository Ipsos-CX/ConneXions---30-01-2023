            {
                // get the file name from the full path and name
                FileInfo sampleFile = new FileInfo(Dts.Variables["SampleFilePathAndName"].Value.ToString());

                Dts.Variables["SampleFileName"].Value = sampleFile.Name;

                // get the contents of the file into a string variable
                string fileContents = File.ReadAllText(Dts.Variables["SampleFilePathAndName"].Value.ToString());

                // now get the hash code for that string and add it to the FileChecksum variable
                Dts.Variables["FileChecksum"].Value = fileContents.GetHashCode();

                // get the number of rows in the file
                string[] fileRows = File.ReadAllLines(Dts.Variables["SampleFilePathAndName"].Value.ToString());
                Dts.Variables["FileRowCount"].Value = fileRows.Length;

                // set the LoadSuccess flag to true - we change this later if it fails
                Dts.Variables["LoadSuccess"].Value = true;

