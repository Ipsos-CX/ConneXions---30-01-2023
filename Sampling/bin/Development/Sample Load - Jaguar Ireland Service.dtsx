        public void Main()
        {
            try
            {
                // get the current datestamp to use in the folder name
                string folderName = GetDateStamp();

                string archiveFolder = Dts.Variables["SamplePath"].Value.ToString() + @"Archive\" + folderName;

                // if the datestamped archive folder does not exist, create it
                if (!Directory.Exists(archiveFolder))
                {
                    Directory.CreateDirectory(archiveFolder);
