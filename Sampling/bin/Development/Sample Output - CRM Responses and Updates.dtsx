		To use the connections collection use something like the following:
		ConnectionManager cm = Dts.Connections.Add("OLEDB");
		cm.ConnectionString = "Data Source=localhost;Initial Catalog=AdventureWorks;Provider=SQLNCLI10;Integrated Security=SSPI;Auto Translate=False;";

		Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
		
		To open Help, press F1.
	*/

        public void Main()
        {
            // get a date time stamped string to use in the output file name

            string dateTimeStamp = "";
            DateTime date;
            string day, month, year, hour, minute;

            date = DateTime.Now;

            day = date.Day.ToString().PadLeft(2, '0');
            month = date.Month.ToString().PadLeft(2, '0');
            year = date.Year.ToString();

            hour = date.Hour.ToString().PadLeft(2, '0');
            minute = date.Minute.ToString().PadLeft(2, '0');

            dateTimeStamp = day + month + year + hour + minute;

            Dts.Variables["DateTimeStampString"].Value = dateTimeStamp;

            Dts.Log("DateTimeStampString: " + dateTimeStamp, 0, null);

            Dts.TaskResult = (int)ScriptResults.Success;
        }
