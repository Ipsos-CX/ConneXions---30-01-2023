            Dts.Log("SamplePath: " + Dts.Variables["SamplePath"].Value.ToString(), 0, null);
            Dts.Log("EventTypeID: " + Dts.Variables["EventTypeID"].Value.ToString(), 0, null);
            
            Dts.TaskResult = (int)ScriptResults.Success;
        }
    }
