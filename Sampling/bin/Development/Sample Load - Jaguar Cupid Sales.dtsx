                      dataType="System.String"><![CDATA[/* Microsoft SQL Server Integration Services Script Component
*  Write scripts using Microsoft Visual C# 2008.
*  ScriptMain is the entry point class of the script.*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.Windows.Forms;

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        try
        {
            // set EventTypeID
            Row.EventTypeID = Int32.Parse(ReadOnlyVariables["EventTypeID"].Value.ToString());

            // set the name capitalisation
            Row.SetNameCapitalisation = false;

        /* MANUFACTURER SPECIFIC INFO */

            if (Row.Manufacturer == "J")
            {
                Row.ManufacturerPartyID = 2;
                Row.DealerCodeOriginatorPartyID = 2;
                Row.SampleSupplierPartyID = 2;
            }
            else if (Row.Manufacturer == "L")
            {
                Row.ManufacturerPartyID = 3;
                Row.DealerCodeOriginatorPartyID = 3;
                Row.SampleSupplierPartyID = 3;
            }

        /* PARTY INFO */

            // set the OrganisationNameOrig value
            Row.OrganisationNameOrig = Row.CompanyName1.Trim() + " " + Row.CompanyName2.Trim();

            // set the GenderID
            if (!Row.Gender_IsNull && Row.Gender == "M")
            {
                Row.GenderID = 1;
            }
            else if (!Row.Gender_IsNull && Row.Gender == "F")
            {
                Row.GenderID = 2;
            }
            else
            {
                Row.GenderID = 0;
            }

        /* ADDRESS INFO */

            // set the StrretOrig and StreetAndNumberOrig
            if (Row.ResidenceCountry == "GBR")
            {
                Row.StreetAndNumberOrig = Row.Address1;
            }
            else
            {
                Row.StreetOrig = Row.Address1;
            }

            // set the Town
            /*
            if (!Row.Address4_IsNull && Row.Address4.Trim().Length > 1)
            {
                Row.Town = Row.Address4.Trim() + " " + Row.Address5.Trim();
            }
            else
            {
                Row.Town = Row.Address5.Trim();
            }
            */

            /* check address fields, if they contain any text concatenate as town */
            string Add4 = Row.Address4 ?? "";
            string Add5 = Row.Address5 ?? "";
            string Town = Add4.Trim() + " " + Add5.Trim();
            Row.Town = Town.Trim();

            // set the postcode
            Row.Postcode = Row.PostcodePrefix.Trim() + " " + Row.PostcodeSuffix.Trim();

        /* PHONE INFO */

        /* EMAIL INFO */

        /* VEHICLE INFO */

            // set VIN usable
            if (!Row.VIN_IsNull && Row.VIN.Length == 17)
            {
                Row.VehicleIdentificationNumberUsable = true;
            }
            else
            {
                Row.VehicleIdentificationNumberUsable = false;
            }

            // set the ownership cycle
            if (!Row.OwnershipCycle_IsNull && Row.OwnershipCycle.Trim().Length > 0)
            {
                Row.ConvertedOwnershipCycle = SByte.Parse(Row.OwnershipCycle.Trim());
            }
            else
            {
                Row.ConvertedOwnershipCycle = 0;
            }

            // set the driver indicator flag
            if (!Row.DriverIndicator_IsNull && Row.DriverIndicator == "Y")
            {
                Row.ConvertedDriverIndicator = true;
            }
            else
            {
                Row.ConvertedDriverIndicator = false;
            }

            // set the owner indicator flag
            if (!Row.OwnerIndicator_IsNull && Row.OwnerIndicator == "Y")
            {
                Row.ConvertedOwnerIndicator = true;
            }
            else
            {
                Row.ConvertedOwnerIndicator = false;
            }

            // set the manager indicator flag
            if (!Row.ManagerIndicator_IsNull && Row.ManagerIndicator == "Y")
            {
                Row.ConvertedManagerIndicator = true;
            }
            else
            {
                Row.ConvertedManagerIndicator = false;
            }



            // Set Industry Classification based on GlobalSalesCode and Country
            Row.SuppliedIndustryClassificationID = 0;
            //if (Row.AFRLCode.Trim() == "P" || Row.AFRLCode.Trim() == "B" || Row.CountryCode.Trim() != "GBR")
            //{
            //    Row.SuppliedIndustryClassificationID = 0;
            //}
            //else
            //{
            //    Row.SuppliedIndustryClassificationID = 3;   //Vehicle Leasing Company
            //}



            // Set suppressions flags based on CupidSupressionCode column
            string caseSwitch = "";
            if (!Row.CupidCustomerSuppressionCode_IsNull && Row.CupidCustomerSuppressionCode.ToString().Length > 0)

            { caseSwitch = Row.CupidCustomerSuppressionCode.ToString(); }

            switch (caseSwitch)
            {
                case "001":
                case "011":
                case "012":
                case "014":
                    Row.PartySuppression = true;               // Full suppression
                    Row.PostalSuppression = true;
                    Row.EmailSuppression = true;
                    break;
                case "015":
                    Row.PartySuppression = false;               // Only email suppression
                    Row.PostalSuppression = false;
                    Row.EmailSuppression = true;
                    break;
                default:
                    Row.EmailSuppression = false;             // Default - No suppressions
                    Row.PostalSuppression = false;
                    Row.PartySuppression = false;
                    break;
            }




        /* CUSTOMER IDENTIFIER INFO */

            // set the customer identifier information
            if (!Row.CustomerID_IsNull && Row.CustomerID.Length > 0)
            {
                if (!Row.ResidenceCountry_IsNull && Row.ResidenceCountry.Length > 0)
                {
                    Row.CustomerIdentifier = Row.CustomerID + "_" + Row.ResidenceCountry;
                    Row.CustomerIdentifierUsable = true;
                }
                else
                {
                    Row.CustomerIdentifier = Row.CustomerID;
                    Row.CustomerIdentifierUsable = false;
                }

                if (Row.Manufacturer == "J")
                {
                    Row.CustomerIdentifierOriginatorPartyID = 2;
                }
                else if (Row.Manufacturer == "L")
                {
                    Row.CustomerIdentifierOriginatorPartyID = 3;
                }
            }

            
        }
        catch (Exception e)
        {
            throw e;
        }

        
    }

}
]]></arrayElement>
