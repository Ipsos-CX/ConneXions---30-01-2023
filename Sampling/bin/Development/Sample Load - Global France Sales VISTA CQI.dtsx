            //    }
            //}


            string caseSwitch = "";
            if (!Row.CommonTypeofSale_IsNull && Row.CommonTypeofSale.ToString().Length > 0)

            { caseSwitch = Row.CommonTypeofSale.ToString(); }

            switch (caseSwitch)
            {
                case "AAA":
                case "FAA":
                case "AGA":     // V1.1
                case "FBB":     // V1.1
                case "FAE":     // V1.2   Azure Task 559 / BUG 18294 - Selection of Fleets of 5 to 24 vehicles for France
                    Row.opIndustryClassificationID = 0;         // Not industry!
                    break;
                case "FAC":
                case "FAD":
                case "FAU":
                case "MAD":
                case "MAH":
                    Row.opIndustryClassificationID = 3;         //Leasing industry
                    break;
                default:
                    Row.opIndustryClassificationID = 2;         //Set everything else Automotive Industry
                    break;
            }


            //BUG 11796 : Vista - Multiple Record Per Vehicle Loader Set-up
            string RoleDescription = Row.ContractRelationship.ToUpper();


            Row.opDriverindicator = 0;
            Row.opOwnerIndicator = 0;
            Row.opManagerIndicator = 0;

            if (RoleDescription == "OWNER")
            {
                Row.opOwnerIndicator = 1;
            }

            if (RoleDescription == "DRIVER")
            {
                Row.opDriverindicator = 1;
            }


            if (RoleDescription == "OWNER AND DRIVER")
            {
                Row.opOwnerIndicator = 1;
                Row.opDriverindicator = 1;
            }

            if (RoleDescription == "FLEET/MANAGER/EMPLOYER COMPANY")
            {
                Row.opManagerIndicator = 1;
            }


            //BUG 14173 - Purchase France - Vista Employee fields
            string strSalesman ="";
            if (!Row.SalesmanFirstName_IsNull && Row.SalesmanFirstName.ToString().Length > 0)
            {
                strSalesman = Row.SalesmanFirstName;
            }    
           
             if (!Row.SalesmanSurname_IsNull && Row.SalesmanSurname.ToString().Length > 0)
            {
                strSalesman = strSalesman + " " + Row.SalesmanSurname;
            }
             Row.opSalesmanName = strSalesman.Trim();


        }
        catch (Exception e)
        {
            throw e;
        }

        
    }

