                      dataType="System.String"><![CDATA[/* Microsoft SQL Server Integration Services Script Component
*  Write scripts using Microsoft Visual C# 2008.
*  ScriptMain is the entry point class of the script.*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.Windows.Forms;

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        try
        {
            if (!Row.CustomerId_IsNull && Row.CustomerId.Trim().Length > 0)
            {
                Row.CustomerIdentifier = Row.CustomerId.Trim() + "_VISTA";
                Row.CustomerIdentifierUsable = true;
                Row.CustomerIdentifierOriginatorPartyID = Int32.Parse(ReadOnlyVariables["DealerCodeOriginatorPartyID"].Value.ToString()); 
            }
            else
            {
                Row.CustomerIdentifierUsable = false;
            }

            if (!Row.VIN_IsNull && Row.VIN.Length == 17)
            {
                Row.VehicleIdentificationNumberUsable = true;
            }
            else
            {
                Row.VehicleIdentificationNumberUsable = false;
            }

            //Row.ManufacturerPartyID = Int32.Parse(ReadOnlyVariables["ManufacturerPartyID"].Value.ToString());
            //Row.SampleSupplierPartyID = Int32.Parse(ReadOnlyVariables["ManufacturerPartyID"].Value.ToString());
            //Row.CountryID = Int16.Parse(ReadOnlyVariables["CountryID"].Value.ToString());
            //Row.EventTypeID = Int16.Parse(ReadOnlyVariables["EventTypeID"].Value.ToString());
            //Row.LanguageID = Int16.Parse(ReadOnlyVariables["LanguageID"].Value.ToString());
            //Row.DealerCodeOriginatorPartyID = Int32.Parse(ReadOnlyVariables["DealerCodeOriginatorPartyID"].Value.ToString());
            //Row.SetNameCapitalisation = Boolean.Parse(ReadOnlyVariables["SetNameCapitalisation"].Value.ToString());

                    
            if (!Row.PartnerUniqueId_IsNull && Row.PartnerUniqueId.Length == 13)
            {
                Row.opDealerCode = Row.PartnerUniqueId.Substring(Row.PartnerUniqueId.Length-5) ;
            }


            //Concatenate address line 4 into 3. Line 4 always seems to be blank but this is just in case.
            if (!Row.Address4_IsNull && Row.Address4.Trim().Length > 0)
            {
                Row.Address3 = Row.Address3.Trim() + " " + Row.Address4.Trim();
            }

            // ----------- Advised not to use Gender field and use Title instead -----------
            //if (!Row.Gender_IsNull && Row.Gender == "M")
            //{
            //       Row.opGenderID = 1;
            //}
            //else if (!Row.Gender_IsNull && Row.Gender == "F")
            //{
            //    Row.opGenderID = 2;
            //}
            //else
            //{
            //    Row.opGenderID = 0;
            //}

            if (!Row.Title_IsNull && Row.Title.Length >= 3 && Row.Title.ToLower().Substring(0, 3) == "mon") //Monsieur
            {
                Row.opGenderID = 1;
            }
            else if (!Row.Title_IsNull && Row.Title.Length >= 3 && Row.Title.ToLower().Substring(0, 3) == "mad") //Mademoiselle + Madame
            {
                Row.opGenderID = 2;
            }
            else
            {
                Row.opGenderID = 0;
            }


            // If either EmailMarketing or Consent to contact set then Supress everything
            if ((!Row.Emailmarketingoptin_IsNull && Row.Emailmarketingoptin == "N")
                | (!Row.ConsentToContact_IsNull && Row.ConsentToContact == "1")
                | ((Row.Emailmarketingoptin_IsNull | Row.Emailmarketingoptin.Trim() == "")
                    && (Row.ConsentToContact_IsNull | Row.ConsentToContact.Trim() == ""))
                )
            {
                Row.opPartySuppression = true;
                Row.opEmailSuppression = true;
                Row.opPostalSuppression = true;
            }
            else
            {
                Row.opEmailSuppression = false;
                Row.opPostalSuppression = false;
                Row.opPartySuppression = false;
            }



            if (!Row.ModelYearDescription_IsNull && Row.ModelYearDescription.Length >= 4)
            { 
                // First 4 chars should be model year but we will check they are numeric //
                int Num;
                bool IsNum = int.TryParse(Row.ModelYearDescription.Substring(0, 4), out Num);

                if (IsNum)
                {
                    Row.opModelYear = Num;
                }
            }

            // Set the Industry Classification based on the Type of Sale
            Row.opIndustryClassificationID = 0;
            //if (!Row.CommonTypeofSale_IsNull)
            //{
            //    if (Row.CommonTypeofSale.ToString() == "AAA")
