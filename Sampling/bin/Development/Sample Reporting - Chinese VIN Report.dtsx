                Name="ScriptMain.cs"
                Encoding="UTF8"><![CDATA[/*
   Microsoft SQL Server Integration Services Script Task
   Write scripts using Microsoft Visual C# 2008.
   The ScriptMain is the entry point class of the script.
*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;

namespace ST_78da772ccb2945cba300ef66a0a48d0f.csproj
{
    [Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        #region VSTA generated code
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

        /*
		The execution engine calls this method when the task executes.
		To access the object model, use the Dts property. Connections, variables, events,
		and logging features are available as members of the Dts property as shown in the following examples.

		To reference a variable, call Dts.Variables["MyCaseSensitiveVariableName"].Value;
		To post a log entry, call Dts.Log("This is my log text", 999, null);
		To fire an event, call Dts.Events.FireInformation(99, "test", "hit the help message", "", 0, true);

		To use the connections collection use something like the following:
		ConnectionManager cm = Dts.Connections.Add("OLEDB");
		cm.ConnectionString = "Data Source=localhost;Initial Catalog=AdventureWorks;Provider=SQLNCLI10;Integrated Security=SSPI;Auto Translate=False;";

		Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
		
		To open Help, press F1.
	*/

        public void Main()
        {
            try
            {
                // if the FTP script process is not enabled we want to terminate the execution
                // set the task failure to failed and then we can write a log in the log tables
                if (!Boolean.Parse(Dts.Variables["FTPDisabled"].Value.ToString()))
                {
                    // if the process is enabled set the FTP connection properties
                    Dts.Connections["FTP Connection Manager"].Properties["ServerName"].SetValue(Dts.Connections["FTP Connection Manager"].InnerObject, Dts.Variables["HostName"].Value.ToString());
                    Dts.Connections["FTP Connection Manager"].Properties["ServerPort"].SetValue(Dts.Connections["FTP Connection Manager"].InnerObject, Dts.Variables["Port"].Value.ToString());
                    Dts.Connections["FTP Connection Manager"].Properties["ServerUserName"].SetValue(Dts.Connections["FTP Connection Manager"].InnerObject, Dts.Variables["UserID"].Value.ToString());
                    Dts.Connections["FTP Connection Manager"].Properties["ServerPassword"].SetValue(Dts.Connections["FTP Connection Manager"].InnerObject, Dts.Variables["Password"].Value.ToString());

                    // log the FTP Connection details
                    Dts.Log("FTP Connection HostName: " + Dts.Variables["HostName"].Value.ToString(), 0, null);
                    Dts.Log("FTP Connection Port: " + Dts.Variables["Port"].Value.ToString(), 0, null);
                    Dts.Log("FTP Connection UserID: " + Dts.Variables["UserID"].Value.ToString(), 0, null);
                    Dts.Log("FTP Connection Password: " + Dts.Variables["Password"].Value.ToString(), 0, null);

                    // set the path to the errors folder
                    Dts.Variables["LocalErrorPath"].Value = Dts.Variables["LocalPath"].Value.ToString() + @"\Errors\";

                    Dts.TaskResult = (int)ScriptResults.Success;
                }
                else
                {
                    // log the fact that the FTP details are disabled
                    Dts.Log("The FTP connection is disabled in FTPMetadata", 0, null);

                    Dts.TaskResult = (int)ScriptResults.Failure;
                }
            }
            catch (Exception e)
            {
                MessageBox.Show(e.Message);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }
    }
}]]></ProjectItem>
