using System;
using System.Data;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
using System.Windows.Forms;

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {

            
        try
        {
            if (!Row.EmailAddress1_IsNull && Row.EmailAddress1.Length > 1)
            {
                Row.ValidatedEmailAddress1 = Row.EmailAddress1;
            }
            else
            {
                Row.ValidatedEmailAddress1 = "";
            }

            if (!Row.EmailAddress2_IsNull && Row.EmailAddress2.Length > 1)
            {
                Row.ValidatedEmailAddress2 = Row.EmailAddress2;
            }
            else
            {
                Row.ValidatedEmailAddress2 = "";
            }
            
            if (!Row.VIN_IsNull && Row.VIN.Length == 17)
            {
                Row.VehicleIdentificationNumberUsable = true;
            }
            else
            {
                Row.VehicleIdentificationNumberUsable = false;
            }


    
            int num1;
            bool bResult = int.TryParse(Row.OwnershipCycle.ToString(), out num1);
            if (bResult == false)
            {
                Row.opOwnershipCycle = 0;
            }
            else
            {
                Row.opOwnershipCycle = num1;
            }
            

            Row.opCompleteSupression = false;
            Row.opSupressionEmail = false;
            Row.opSupressionMail = false;
            Row.opSupressionPhone = false;

            if (Row.CompleteSuppression.ToUpper() == "YES")
            {
                Row.opCompleteSupression = true;
            }

            if (Row.SuppressionEmail.ToUpper() == "YES" || Row.CompleteSuppression.ToUpper() == "YES")
            {
                Row.opSupressionEmail = true;
            }

            if (Row.SuppressionMail.ToUpper() == "YES" || Row.CompleteSuppression.ToUpper() == "YES")
            {
                Row.opSupressionMail = true;
            }

            if (Row.SuppressionPhone.ToUpper() == "YES" || Row.CompleteSuppression.ToUpper() == "YES")
            {
                Row.opSupressionPhone = true;
            }


            if (!Row.Gender_IsNull && Row.Gender.Length > 0)
            {
                if (Row.Gender.Substring(0, 1).ToUpper() == "M")
                { Row.ConvertedGender = 1; }

                else if (Row.Gender.Substring(0, 1).ToUpper() == "F")
                { Row.ConvertedGender = 2; }

                else { Row.ConvertedGender = 0; }
            }
            else { Row.ConvertedGender = 0; }
        

        }
        catch (Exception e)
        {
            MessageBox.Show(e.Message);
        }

        
    }

}
