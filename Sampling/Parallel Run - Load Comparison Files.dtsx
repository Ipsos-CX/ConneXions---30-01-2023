                Dts.Variables["FileName"].Value = file.Name;

                // log the variables
                Dts.Log("FileName: " + Dts.Variables["FileName"].Value.ToString(), 0, null);
                Dts.Connections["Vehicle"].ConnectionString = Dts.Variables["FilePathAndName"].Value.ToString();

