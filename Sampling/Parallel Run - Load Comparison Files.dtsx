                Dts.Variables["FileName"].Value = file.Name;

                // log the variables
                Dts.Log("FileName: " + Dts.Variables["FileName"].Value.ToString(), 0, null);
                Dts.Connections["CustomerUpdate_Person"].ConnectionString = Dts.Variables["FilePathAndName"].Value.ToString();

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch
